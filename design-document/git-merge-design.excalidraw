    {
      "id": "i9_8-lv0BbrnzsPCJAQ93",
      "type": "text",
      "x": 190.95035343942297,
      "y": 2591.863408959643,
      "width": 1384.7796630859375,
      "height": 2325,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
